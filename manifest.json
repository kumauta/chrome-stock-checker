{
  "name": "Yodobashi Stock Checker",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "yodobashi.comの検索結果に指定店舗の在庫を表示します。",
  "content_scripts": [
    {
      "matches": ["http://www.yodobashi.com/*"],
      "js": ["jquery-2.1.4.js","script.js"],
      "run_at": "document_start"
    }
  ]
}
